{
  "name": "colibri",
  "private": false,
  "description": "ajax proxy v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch": "run-p watch:lib watch:page",
    "watch:lib": "pnpm -C ./packages/colibri-lib watch",
    "watch:page": "pnpm -C ./packages/shell-chrome watch",
    "build": "pnpm -C ./packages/colibri-lib build & pnpm -C ./packages/shell-chrome build",
    "build:lib": "pnpm -C ./packages/colibri-lib build",
    "clean:build": "rm -rf ./packages/colibri-lib/lib && rm -rf ./packages/shell-chrome/dist",
    "page": "pnpm -C ./packages/shell-chrome dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g0ngjie/colibri.git"
  },
  "keywords": [],
